<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <ListPreference
        android:defaultValue="@string/conn_type_val"
        android:entries="@array/conn_types"
        android:entryValues="@array/conn_types"
        android:key="@string/conn_type_key"
        android:title="@string/conn_type_title"/>
    <EditTextPreference
        android:defaultValue="@string/cellmate_server_addr_val"
        android:key="@string/cellmate_server_addr_key"
        android:title="@string/cellmate_server_addr_title"/>
    <EditTextPreference
        android:defaultValue="@string/cellmate_server_port_val"
        android:key="@string/cellmate_server_port_key"
        android:numeric="integer"
        android:title="@string/cellmate_server_port_title"/>
    <EditTextPreference
        android:defaultValue="@string/bosswave_router_addr_val"
        android:key="@string/bosswave_router_addr_key"
        android:title="@string/bosswave_router_addr_title"/>
    <EditTextPreference
        android:defaultValue="@string/bosswave_router_port_val"
        android:key="@string/bosswave_router_port_key"
        android:numeric="integer"
        android:title="@string/bosswave_router_port_title"/>
    <EditTextPreference
        android:defaultValue="@string/bosswave_key_base64_val"
        android:key="@string/bosswave_key_base64_key"
        android:title="@string/bosswave_key_base64_title"/>
    <EditTextPreference
        android:defaultValue="@string/camera_width_val"
        android:enabled="false"
        android:key="@string/camera_width_key"
        android:numeric="integer"
        android:title="@string/camera_width_title"/>
    <EditTextPreference
        android:defaultValue="@string/camera_height_val"
        android:enabled="false"
        android:key="@string/camera_height_key"
        android:numeric="integer"
        android:title="@string/camera_height_title"/>
    <EditTextPreference
        android:defaultValue="@string/camera_fx_val"
        android:key="@string/camera_fx_key"
        android:numeric="decimal"
        android:title="@string/camera_fx_title"/>
    <EditTextPreference
        android:defaultValue="@string/camera_fy_val"
        android:key="@string/camera_fy_key"
        android:numeric="decimal"
        android:title="@string/camera_fy_title"/>
    <EditTextPreference
        android:defaultValue="@string/camera_cx_val"
        android:key="@string/camera_cx_key"
        android:numeric="decimal"
        android:title="@string/camera_cx_title"/>
    <EditTextPreference
        android:defaultValue="@string/camera_cy_val"
        android:key="@string/camera_cy_key"
        android:numeric="decimal"
        android:title="@string/camera_cy_title"/>
</PreferenceScreen>